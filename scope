--This script is open sourced, idm if you use it for your script just give credit!

local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/polar"))()
local ok = library:CreateWindow("Doomware - Noscope Arcade")
ok:Section("Aimbot")
ok:Toggle("AIMBOT",function(a)
    getgenv().Settings = {
   Fov = 200,
   Hitbox = "Head",
   FovCircle = true,
}
 
loadstring(game:HttpGet("https://raw.githubusercontent.com/buqs9999/noscope-arcade-script/main/scope"))()
end)

ok:Toggle("esp",function(a)
 loadstring(game:HttpGet("https://raw.githubusercontent.com/buqs9999/cool-esp-cock/main/esp"))()
end)
